// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Event {
  id            Int     @id @default(autoincrement())
  name          String
  eventDates    String
  image         String? 
  place         String
  website       String?
  organizer     String
  phone         String
  email         String
  linkedin      String?
  description   String
}